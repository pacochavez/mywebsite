body	
	.main
		.contact
			text-align center
			.img
				width 150px
				height 150px
				-webkit-border-radius 75px
				border-radius 75px
				overflow hidden
				display inline-block
				img
					width 100%
			ul
				display block
				padding 0px
				li
					background gris_medio
					display inline-block
					list-style none
					width 30px
					height 30px
					border-radius 15px
					margin 2px
					transition all 0.4s ease-in-out
					a
						display block
						color icon_timeline
						height 100%
						text-decoration none						
						&::before
							content ""
							height 100%
							width 0px
							display inline-block
							vertical-align middle
						i
							display inline-block
							vertical-align middle
